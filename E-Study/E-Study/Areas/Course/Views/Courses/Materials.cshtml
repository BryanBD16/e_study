@model CourseMaterialsVM

@{
    ViewData["Title"] = "Materials";
}

<h1>@Model.Title</h1>

<iframe width="560" height="315"
        src="https://www.youtube.com/embed/@Model.YouTubeId"
        frameborder="0"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen>
</iframe>

<a asp-controller="Questions" asp-action="Evaluation" asp-route-CourseId="@Model.Id">Go to the Evaluation</a>